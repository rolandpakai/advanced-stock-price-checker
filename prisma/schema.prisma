generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model StockQuote {
  id         Int      @id @default(autoincrement())
  symbol     String
  currentPrice      Float
  timestamp  DateTime
  createdAt  DateTime @default(now())

  @@index([symbol])
}
